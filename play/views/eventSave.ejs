      "gasUsed": 46997,
      "logIndex": 2,
      "transactionHash": "0x8b4352037c528b4655fb975feb3d3d3d2aa217a4730ed33fe6d83c902cbce34c",
      "transactionIndex": 3
    };

    eventSave(_data);
  }

  function eventSave(_data) {
    var _eshelper = new EventHelper();
    var _logs = decodeEventsForContract(abiGP, [_data]);
    console.log('decodeEventsForContract:',_logs);
    _eshelper.saveEvent(_logs[0]);
    appendResult(_logs[0]);
  }


  // var Web3 = require('web3');
  $(document).ready(function() {
    var code = runCode.toString()+"\n\n";
    code += eventSave.toString() + "\n\n";
    code += "runCode();\n\n";
    setCode(code);

  });



</script>
